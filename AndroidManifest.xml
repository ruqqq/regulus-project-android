<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="sg.rp.geeks.leoapp"
      android:versionCode="3"
      android:versionName="1.2">
    <application
            android:name="LeoApp"
            android:label="@string/app_name"
            android:icon="@drawable/icon"
            android:theme="@style/Theme.LeoApp">
        <activity android:name="DashboardActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="TimetableActivity"
                  android:label="@string/timetable"
                  android:screenOrientation="portrait"></activity>
        <activity android:name="GradesActivity"
                  android:label="@string/recent_grades"
                  android:screenOrientation="portrait"></activity>
        <activity android:name="ModuleSummaryActivity"
                  android:label="@string/module_summary"
                  android:screenOrientation="portrait"></activity>
        <activity android:name="ProfileActivity"
                  android:label="@string/profile"
                  android:screenOrientation="portrait"></activity>

        <activity android:name="PreferencesActivity"
                  android:label="@string/app_preferences"/>
    </application>
    <uses-sdk android:minSdkVersion="4" />
    <uses-permission android:name="android.permission.INTERNET" />
</manifest> 
